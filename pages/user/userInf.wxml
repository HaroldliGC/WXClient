  <view class="InfGroup">
    <navigator url="./userName?userId={{user.Id}}">
    <Cell
      title='昵称' 
      content='{{user.Name}}'
    >
    </Cell>
    </navigator>
    <Cell
      title='账户号' 
      content='{{user.Account}}'
    >
    </Cell>
    <Cell
      title='邮箱' 
      content='{{user.Email}}'
    >
    </Cell>
     <Cell
      title='证件号' 
      content='{{user.License}}'
    >
    </Cell>
  </view>
  <view class="blank"></view>
  <view class="InfGroup">
    <navigator url="./userMore?userId={{user.Id}}">
    <Cell
      title='更多' 
      content=''
    >
    </Cell>
    </navigator>
  </view>
  <Navigation></Navigation>
</view>

